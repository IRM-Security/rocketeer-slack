{
	"name": "rocketeers/rocketeer-slack",
	"description": "Slack plugin for Rocketeer",
	"license": "MIT",
	"keywords": [
		"rocketeer",
		"deployment",
		"slack",
		"laravel"
	],
	"authors": [
		{
			"name": "Maxime Fabre",
			"email": "ehtnam6@gmail.com"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/IRM-Security/rocketeer.git"
		},
		{
			"type": "vcs",
			"url": "https://github.com/IRM-Security/rocketeer-remote.git"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"illuminate/support": "~4.2",
		"rocketeers/rocketeer": "dev-master",
		"maknz/slack": "^1.7"
	},
	"autoload": {
		"psr-4": {
			"Rocketeer\\Plugins\\Slack\\": "src/Slack"
		}
	},
	"minimum-stability": "dev",
	"config": {
		"allow-plugins": {
			"kylekatarnls/update-helper": false
		}
	}
}
